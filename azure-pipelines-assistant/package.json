{
  "name": "azure-pipelines-assistant",
  "displayName": "Azure Pipelines Assistant",
  "description": "Manage Azure DevOps pipelines directly from VS Code",
  "version": "0.0.1",
  "publisher": "azure-pipelines-assistant",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/azure-pipelines-assistant/azure-pipelines-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/azure-pipelines-assistant/azure-pipelines-assistant/issues"
  },
  "homepage": "https://github.com/azure-pipelines-assistant/azure-pipelines-assistant#readme",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Azure",
    "Other"
  ],
  "keywords": [
    "azure",
    "devops",
    "pipelines",
    "ci/cd",
    "build"
  ],
  "activationEvents": [
    "onView:azurePipelinesView",
    "onCommand:azurePipelinesAssistant.configure",
    "onCommand:azurePipelinesAssistant.runHealthCheck",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "azurePipelinesContainer",
          "title": "Azure Pipelines",
          "icon": "$(cloud)"
        }
      ]
    },
    "views": {
      "azurePipelinesContainer": [
        {
          "id": "azurePipelinesView",
          "name": "Pipelines",
          "when": "true"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "azurePipelinesView",
        "contents": "Welcome to Azure Pipelines Assistant!\n\nTo get started, configure your Azure DevOps organization and Personal Access Token.\n\n[Configure Extension](command:azurePipelinesAssistant.configure)\n\n[Show Welcome Guide](command:azurePipelinesAssistant.showWelcome)\n\nNeed help? [View Documentation](https://docs.microsoft.com/en-us/azure/devops/integrate/get-started/authentication/pats)",
        "when": "!azurePipelinesAssistant.configured"
      }
    ],
    "commands": [
      {
        "command": "azurePipelinesAssistant.configure",
        "title": "Configure Azure Pipelines Assistant",
        "icon": "$(gear)"
      },
      {
        "command": "azurePipelinesAssistant.refresh",
        "title": "Refresh Azure Pipelines Assistant",
        "icon": "$(refresh)"
      },
      {
        "command": "azurePipelinesAssistant.runPipeline",
        "title": "Run Pipeline",
        "icon": "$(play)"
      },
      {
        "command": "azurePipelinesAssistant.viewInBrowser",
        "title": "View in Browser",
        "icon": "$(link-external)"
      },
      {
        "command": "azurePipelinesAssistant.viewRunDetails",
        "title": "View Run Details",
        "icon": "$(info)"
      },
      {
        "command": "azurePipelinesAssistant.viewLogs",
        "title": "View Logs",
        "icon": "$(output)"
      },
      {
        "command": "azurePipelinesAssistant.downloadArtifacts",
        "title": "Download Artifacts",
        "icon": "$(cloud-download)"
      },
      {
        "command": "azurePipelinesAssistant.addToFavorites",
        "title": "Add to Favorites",
        "icon": "$(star-empty)"
      },
      {
        "command": "azurePipelinesAssistant.removeFromFavorites",
        "title": "Remove from Favorites",
        "icon": "$(star-full)"
      },
      {
        "command": "azurePipelinesAssistant.viewRecentRuns",
        "title": "View Recent Runs",
        "icon": "$(history)"
      },
      {
        "command": "azurePipelinesAssistant.cancelRun",
        "title": "Cancel Run",
        "icon": "$(stop)"
      },
      {
        "command": "azurePipelinesAssistant.approveRun",
        "title": "Approve Run",
        "icon": "$(check)"
      },
      {
        "command": "azurePipelinesAssistant.rejectRun",
        "title": "Reject Run",
        "icon": "$(x)"
      },
      {
        "command": "azurePipelinesAssistant.monitorRun",
        "title": "Monitor Run",
        "icon": "$(eye)"
      },
      {
        "command": "azurePipelinesAssistant.stopMonitoring",
        "title": "Stop Monitoring",
        "icon": "$(eye-closed)"
      },
      {
        "command": "azurePipelinesAssistant.compareRuns",
        "title": "Compare Runs",
        "icon": "$(diff)"
      },
      {
        "command": "azurePipelinesAssistant.resetConfiguration",
        "title": "Reset Configuration",
        "icon": "$(refresh)"
      },
      {
        "command": "azurePipelinesAssistant.exportConfiguration",
        "title": "Export Configuration",
        "icon": "$(export)"
      },
      {
        "command": "azurePipelinesAssistant.importConfiguration",
        "title": "Import Configuration",
        "icon": "$(import)"
      },
      {
        "command": "azurePipelinesAssistant.openSettings",
        "title": "Open Settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "azurePipelinesAssistant.validateConfiguration",
        "title": "Validate Configuration",
        "icon": "$(check)"
      },
      {
        "command": "azurePipelinesAssistant.showWelcome",
        "title": "Show Welcome",
        "icon": "$(home)"
      },
      {
        "command": "azurePipelinesAssistant.runHealthCheck",
        "title": "Run Health Check",
        "icon": "$(pulse)"
      },
      {
        "command": "azurePipelinesAssistant.showMemoryDiagnostics",
        "title": "Show Memory Diagnostics",
        "icon": "$(graph)"
      },
      {
        "command": "azurePipelinesAssistant.showExtensionDiagnostics",
        "title": "Show Extension Diagnostics",
        "icon": "$(info)"
      },
      {
        "command": "azurePipelinesAssistant.clearCache",
        "title": "Clear Cache",
        "icon": "$(clear-all)"
      },
      {
        "command": "azurePipelinesAssistant.resetExtensionState",
        "title": "Reset Extension State",
        "icon": "$(refresh)"
      },
      {
        "command": "azurePipelinesAssistant.exportDiagnostics",
        "title": "Export Diagnostics",
        "icon": "$(export)"
      },
      {
        "command": "azurePipelinesAssistant.showActivationMetrics",
        "title": "Show Activation Metrics",
        "icon": "$(dashboard)"
      },
      {
        "command": "azurePipelinesAssistant.searchPipelines",
        "title": "Search Pipelines",
        "icon": "$(search)"
      },
      {
        "command": "azurePipelinesAssistant.filterByProject",
        "title": "Filter by Project",
        "icon": "$(filter)"
      },
      {
        "command": "azurePipelinesAssistant.filterByStatus",
        "title": "Filter by Status",
        "icon": "$(filter)"
      },
      {
        "command": "azurePipelinesAssistant.clearFilters",
        "title": "Clear Filters",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "azurePipelinesAssistant.refresh",
          "when": "view == azurePipelinesView",
          "group": "navigation"
        },
        {
          "command": "azurePipelinesAssistant.configure",
          "when": "view == azurePipelinesView",
          "group": "navigation"
        },
        {
          "command": "azurePipelinesAssistant.searchPipelines",
          "when": "view == azurePipelinesView && azurePipelinesAssistant.configured",
          "group": "1_search"
        },
        {
          "command": "azurePipelinesAssistant.filterByProject",
          "when": "view == azurePipelinesView && azurePipelinesAssistant.configured",
          "group": "1_search"
        },
        {
          "command": "azurePipelinesAssistant.filterByStatus",
          "when": "view == azurePipelinesView && azurePipelinesAssistant.configured",
          "group": "1_search"
        },
        {
          "command": "azurePipelinesAssistant.clearFilters",
          "when": "view == azurePipelinesView && azurePipelinesAssistant.configured",
          "group": "1_search"
        }
      ],
      "view/item/context": [
        {
          "command": "azurePipelinesAssistant.runPipeline",
          "when": "view == azurePipelinesView && viewItem == pipeline",
          "group": "inline"
        },
        {
          "command": "azurePipelinesAssistant.viewInBrowser",
          "when": "view == azurePipelinesView && viewItem =~ /^(project|pipeline|run)$/",
          "group": "navigation"
        },
        {
          "command": "azurePipelinesAssistant.addToFavorites",
          "when": "view == azurePipelinesView && viewItem =~ /^(project|pipeline)$/ && !azurePipelinesAssistant.isFavorite",
          "group": "favorites"
        },
        {
          "command": "azurePipelinesAssistant.removeFromFavorites",
          "when": "view == azurePipelinesView && viewItem =~ /^(project|pipeline)$/ && azurePipelinesAssistant.isFavorite",
          "group": "favorites"
        },
        {
          "command": "azurePipelinesAssistant.viewRecentRuns",
          "when": "view == azurePipelinesView && viewItem == pipeline",
          "group": "pipeline"
        },
        {
          "command": "azurePipelinesAssistant.compareRuns",
          "when": "view == azurePipelinesView && viewItem =~ /^(pipeline|run)$/",
          "group": "pipeline"
        },
        {
          "command": "azurePipelinesAssistant.viewRunDetails",
          "when": "view == azurePipelinesView && viewItem == run",
          "group": "run"
        },
        {
          "command": "azurePipelinesAssistant.viewLogs",
          "when": "view == azurePipelinesView && viewItem == run",
          "group": "run"
        },
        {
          "command": "azurePipelinesAssistant.downloadArtifacts",
          "when": "view == azurePipelinesView && viewItem == run",
          "group": "run"
        },
        {
          "command": "azurePipelinesAssistant.cancelRun",
          "when": "view == azurePipelinesView && viewItem == run && azurePipelinesAssistant.canCancel",
          "group": "run"
        },
        {
          "command": "azurePipelinesAssistant.approveRun",
          "when": "view == azurePipelinesView && viewItem == run && azurePipelinesAssistant.needsApproval",
          "group": "approval"
        },
        {
          "command": "azurePipelinesAssistant.rejectRun",
          "when": "view == azurePipelinesView && viewItem == run && azurePipelinesAssistant.needsApproval",
          "group": "approval"
        },
        {
          "command": "azurePipelinesAssistant.monitorRun",
          "when": "view == azurePipelinesView && viewItem == run && !azurePipelinesAssistant.isMonitoring",
          "group": "monitoring"
        },
        {
          "command": "azurePipelinesAssistant.stopMonitoring",
          "when": "view == azurePipelinesView && viewItem == run && azurePipelinesAssistant.isMonitoring",
          "group": "monitoring"
        }
      ],
      "commandPalette": [
        {
          "command": "azurePipelinesAssistant.configure",
          "title": "Azure Pipelines: Configure"
        },
        {
          "command": "azurePipelinesAssistant.refresh",
          "title": "Azure Pipelines: Refresh"
        },
        {
          "command": "azurePipelinesAssistant.runPipeline",
          "title": "Azure Pipelines: Run Pipeline",
          "when": "false"
        },
        {
          "command": "azurePipelinesAssistant.viewInBrowser",
          "title": "Azure Pipelines: View in Browser",
          "when": "false"
        },
        {
          "command": "azurePipelinesAssistant.resetConfiguration",
          "title": "Azure Pipelines: Reset Configuration"
        },
        {
          "command": "azurePipelinesAssistant.exportConfiguration",
          "title": "Azure Pipelines: Export Configuration"
        },
        {
          "command": "azurePipelinesAssistant.importConfiguration",
          "title": "Azure Pipelines: Import Configuration"
        },
        {
          "command": "azurePipelinesAssistant.openSettings",
          "title": "Azure Pipelines: Open Settings"
        },
        {
          "command": "azurePipelinesAssistant.validateConfiguration",
          "title": "Azure Pipelines: Validate Configuration"
        },
        {
          "command": "azurePipelinesAssistant.showWelcome",
          "title": "Azure Pipelines: Show Welcome"
        },
        {
          "command": "azurePipelinesAssistant.runHealthCheck",
          "title": "Azure Pipelines: Run Health Check"
        },
        {
          "command": "azurePipelinesAssistant.showMemoryDiagnostics",
          "title": "Azure Pipelines: Show Memory Diagnostics"
        },
        {
          "command": "azurePipelinesAssistant.showExtensionDiagnostics",
          "title": "Azure Pipelines: Show Extension Diagnostics"
        },
        {
          "command": "azurePipelinesAssistant.clearCache",
          "title": "Azure Pipelines: Clear Cache"
        },
        {
          "command": "azurePipelinesAssistant.resetExtensionState",
          "title": "Azure Pipelines: Reset Extension State"
        },
        {
          "command": "azurePipelinesAssistant.exportDiagnostics",
          "title": "Azure Pipelines: Export Diagnostics"
        },
        {
          "command": "azurePipelinesAssistant.showActivationMetrics",
          "title": "Azure Pipelines: Show Activation Metrics"
        },
        {
          "command": "azurePipelinesAssistant.searchPipelines",
          "title": "Azure Pipelines: Search Pipelines"
        },
        {
          "command": "azurePipelinesAssistant.filterByProject",
          "title": "Azure Pipelines: Filter by Project"
        },
        {
          "command": "azurePipelinesAssistant.filterByStatus",
          "title": "Azure Pipelines: Filter by Status"
        },
        {
          "command": "azurePipelinesAssistant.clearFilters",
          "title": "Azure Pipelines: Clear Filters"
        }
      ]
    },
    "configuration": {
      "title": "Azure Pipelines Assistant",
      "properties": {
        "azurePipelinesAssistant.organization": {
          "type": "string",
          "description": "Azure DevOps organization name (e.g., 'myorg' for https://dev.azure.com/myorg)",
          "pattern": "^[a-zA-Z0-9][a-zA-Z0-9-]*[a-zA-Z0-9]$",
          "patternErrorMessage": "Organization name must contain only letters, numbers, and hyphens, and cannot start or end with a hyphen",
          "order": 1
        },
        "azurePipelinesAssistant.personalAccessToken": {
          "type": "string",
          "description": "Personal Access Token for Azure DevOps (Note: This setting is deprecated - tokens are now stored securely)",
          "deprecationMessage": "This setting is deprecated. Use the 'Configure Azure DevOps' command to set your token securely.",
          "order": 2
        },
        "azurePipelinesAssistant.refreshInterval": {
          "type": "number",
          "default": 30,
          "minimum": 10,
          "maximum": 300,
          "description": "Auto-refresh interval in seconds for pipeline data",
          "order": 3
        },
        "azurePipelinesAssistant.maxRunsPerPipeline": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "maximum": 50,
          "description": "Maximum number of recent runs to display per pipeline",
          "order": 4
        },
        "azurePipelinesAssistant.showTimestamps": {
          "type": "boolean",
          "default": true,
          "description": "Show timestamps in logs and run details",
          "order": 5
        },
        "azurePipelinesAssistant.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic refresh of pipeline data in the background",
          "order": 6
        },
        "azurePipelinesAssistant.favoriteProjects": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "List of favorite project IDs for quick access",
          "order": 7
        },
        "azurePipelinesAssistant.favoritePipelines": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "projectId": {
                "type": "string"
              },
              "pipelineId": {
                "type": "number"
              },
              "name": {
                "type": "string"
              }
            },
            "required": [
              "projectId",
              "pipelineId",
              "name"
            ]
          },
          "default": [],
          "description": "List of favorite pipelines for quick access",
          "order": 8
        },
        "azurePipelinesAssistant.cacheTimeout": {
          "type": "number",
          "default": 300,
          "minimum": 60,
          "maximum": 3600,
          "description": "Cache timeout in seconds for API responses",
          "order": 9
        },
        "azurePipelinesAssistant.logLevel": {
          "type": "string",
          "enum": [
            "error",
            "warn",
            "info",
            "debug"
          ],
          "default": "info",
          "description": "Logging level for extension diagnostics",
          "order": 10
        },
        "azurePipelinesAssistant.showWelcomeOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Show welcome message when extension starts and is not configured",
          "order": 11
        },
        "azurePipelinesAssistant.compactView": {
          "type": "boolean",
          "default": false,
          "description": "Use compact view in the pipeline tree to show more items",
          "order": 12
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build:production",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run build:production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "node run-specific-tests.js",
    "test:pipeline-trigger": "node run-pipeline-trigger-tests.js",
    "test:comprehensive": "npm run compile-tests && node out/test/comprehensive/index.js",
    "test:unit": "npm run compile-tests && cross-env RUN_UNIT_TESTS=true RUN_INTEGRATION_TESTS=false RUN_E2E_TESTS=false RUN_PERFORMANCE_TESTS=false node out/test/comprehensive/index.js",
    "test:integration": "npm run compile-tests && cross-env RUN_UNIT_TESTS=false RUN_INTEGRATION_TESTS=true RUN_E2E_TESTS=false RUN_PERFORMANCE_TESTS=false node out/test/comprehensive/index.js",
    "test:e2e": "npm run compile-tests && cross-env RUN_UNIT_TESTS=false RUN_INTEGRATION_TESTS=false RUN_E2E_TESTS=true RUN_PERFORMANCE_TESTS=false node out/test/comprehensive/index.js",
    "test:performance": "npm run compile-tests && cross-env RUN_UNIT_TESTS=false RUN_INTEGRATION_TESTS=false RUN_E2E_TESTS=false RUN_PERFORMANCE_TESTS=true node out/test/comprehensive/index.js",
    "test:performance:extended": "npm run compile-tests && cross-env RUN_PERFORMANCE_TESTS=true PERFORMANCE_ITERATIONS=100 TEST_TIMEOUT=300000 node out/test/comprehensive/index.js",
    "test:coverage": "npm run compile-tests && c8 --reporter=lcov --reporter=text --reporter=html node out/test/comprehensive/index.js",
    "test:watch": "npm run compile-tests && npm-run-all -p watch-tests test:unit",
    "test:debug": "npm run compile-tests && node --inspect-brk out/test/comprehensive/index.js",
    "test:memory": "npm run compile-tests && cross-env NODE_OPTIONS='--max-old-space-size=4096' node out/test/performance/memoryUsage.test.js",
    "test:stress": "npm run compile-tests && cross-env STRESS_TEST=true node out/test/comprehensive/index.js",
    "build": "node scripts/build.js",
    "build:development": "node scripts/build.js --development",
    "build:production": "node scripts/build.js --production",
    "build:watch": "node scripts/build.js --watch",
    "build:analyze": "node esbuild.js --production --analyze",
    "package:vscode": "@vscode/vsce package --out packages/",
    "package:ovsx": "ovsx package -o packages/",
    "package:all": "npm run package:cross-platform",
    "package:cross-platform": "node scripts/package-cross-platform.js",
    "publish:vscode": "vsce publish",
    "publish:ovsx": "ovsx publish",
    "publish:all": "npm run publish:vscode && npm run publish:ovsx",
    "version:info": "node scripts/version-manager.js info",
    "version:patch": "node scripts/version-manager.js bump --patch",
    "version:minor": "node scripts/version-manager.js bump --minor",
    "version:major": "node scripts/version-manager.js bump --major",
    "version:prerelease": "node scripts/version-manager.js bump --prerelease",
    "test:compatibility": "node scripts/test-compatibility.js",
    "test:vscode": "npm run test",
    "test:cursor": "echo 'Testing Cursor IDE compatibility...' && npm run test",
    "test:windsurf": "echo 'Testing Windsurf IDE compatibility...' && npm run test",
    "validate:cross-platform": "node scripts/validate-platforms.js",
    "validate:windows": "echo 'Validating Windows compatibility...' && npm run package",
    "validate:macos": "echo 'Validating macOS compatibility...' && npm run package",
    "validate:linux": "echo 'Validating Linux compatibility...' && npm run package",
    "ci:test": "npm run test:comprehensive",
    "ci:test:unit": "npm run test:unit",
    "ci:test:integration": "npm run test:integration",
    "ci:test:e2e": "npm run test:e2e",
    "ci:test:performance": "npm run test:performance",
    "ci:coverage": "npm run test:coverage",
    "ci:build": "node scripts/build.js --production --with-tests",
    "ci:package": "npm run package:cross-platform",
    "clean": "rimraf dist out packages coverage .nyc_output",
    "clean:all": "npm run clean && rimraf node_modules"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.2.1",
    "c8": "^10.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "glob": "^11.0.3",
    "madge": "^8.0.0",
    "mocha": "^10.8.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "ovsx": "^0.9.10",
    "rimraf": "^6.0.1",
    "sinon": "^21.0.0",
    "typescript": "^5.8.3"
  }
}